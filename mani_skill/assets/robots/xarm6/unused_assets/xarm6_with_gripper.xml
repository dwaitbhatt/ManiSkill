<mujoco model="xarm6">

  <actuator>
    <!-- Physical limits of the actuator. -->
    <motor ctrllimited="true" ctrlrange="-50.0 50.0" joint="joint1" name="torq_j1"/>
    <motor ctrllimited="true" ctrlrange="-50.0 50.0" joint="joint2" name="torq_j2"/>
    <!-- <motor ctrllimited="true" ctrlrange="-32.0 32.0" joint="joint3" name="torq_j3"/> -->
    <!-- <motor ctrllimited="true" ctrlrange="-32.0 32.0" joint="joint4" name="torq_j4"/> -->
    <!-- <motor ctrllimited="true" ctrlrange="-32.0 32.0" joint="joint5" name="torq_j5"/> -->
    <motor ctrllimited="true" ctrlrange="-50.0 50.0" joint="joint3" name="torq_j3"/>
    <motor ctrllimited="true" ctrlrange="-50.0 50.0" joint="joint4" name="torq_j4"/>
    <motor ctrllimited="true" ctrlrange="-50.0 50.0" joint="joint5" name="torq_j5"/>
    <motor ctrllimited="true" ctrlrange="-20.0 20.0" joint="joint6" name="torq_j6"/>

    <position name='finger_1' ctrllimited="true" kp="2" joint='drive_joint' ctrlrange='0 0.85'/>
    <position name='finger_2' ctrllimited="true" kp="2" joint='right_outer_knuckle_joint' ctrlrange='0 0.85'/>
  </actuator>

  <asset>
    <mesh name="base_v" file="visual/base.stl"/>
    <!-- <mesh name="base_c" file="collision/base_vhacd.obj"/> -->
    <mesh name="base_c" file="visual/base.stl"/>
    
    <mesh name="link1_v" file="visual/link1.stl"/>
    <!-- <mesh name="link1_c" file="collision/link1_vhacd.obj"/> -->
    <mesh name="link1_c" file="visual/link1.stl"/>
    
    <mesh name="link2_v" file="visual/link2.stl"/>
    <!-- <mesh name="link2_c" file="collision/link2_vhacd2.obj"/> -->
    <mesh name="link2_c" file="visual/link2.stl"/>
    
    <mesh name="link3_v" file="visual/link3.stl"/>
    <!-- <mesh name="link3_c" file="collision/link3_vhacd.obj"/> -->
    <mesh name="link3_c" file="visual/link3.stl"/>
    
    <mesh name="link4_v" file="visual/link4.stl"/>
