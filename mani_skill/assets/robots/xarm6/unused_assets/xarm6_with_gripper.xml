    <!-- <mesh name="link6_c" file="collision/link6_vhacd.obj"/> -->
    <mesh name="link6_c" file="visual/link6.stl"/>
    
    <mesh name="base_link" file="meshes_xarm_gripper/base_link.STL"/>
    <mesh name="base_link_collision" file="meshes_xarm_gripper/base_link_collision.STL"/>
    <mesh name="left_outer_knuckle" file="meshes_xarm_gripper/left_outer_knuckle.STL"/>
    <mesh name="left_finger" file="meshes_xarm_gripper/left_finger.STL"/>
    <mesh name="left_inner_knuckle" file="meshes_xarm_gripper/left_inner_knuckle.STL"/>
    <mesh name="right_outer_knuckle" file="meshes_xarm_gripper/right_outer_knuckle.STL"/>
    <mesh name="right_finger" file="meshes_xarm_gripper/right_finger.STL"/>
    <mesh name="right_inner_knuckle" file="meshes_xarm_gripper/right_inner_knuckle.STL"/>
  </asset>

  <tendon>
    <!--finger2 tendons-->
    <fixed name="finger2_12_cpl" range="-1 1" stiffness="0.1" springlength="0.1" limited="true">
      <joint joint="drive_joint"  coef="1"/>
      <joint joint="left_finger_joint"  coef="-3"/>
      <joint joint="left_inner_knuckle_joint"  coef="-1"/>
    </fixed>

    <!--Finger1 tendons-->
    <fixed name="finger1_12_cpl" range="-1 1" stiffness="0.1" springlength="0.1" limited="true">
      <joint joint="right_outer_knuckle_joint"  coef="1"/>
      <joint joint="right_finger_joint"  coef="-3"/>
      <joint joint="right_inner_knuckle_joint"  coef="-1"/>
    </fixed>
  </tendon>

